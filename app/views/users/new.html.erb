<%= link_to "Login", login_path %><br><br>


<%= form_for(@user, url: signup_path) do |f| %>
    <%= f.label :email %>
    <%= f.text_field :email %>
    <%= f.label :password %>
    <%= f.text_field :password %>

    <%= f.submit "Sign Up" %>
<% end %>


<style>
body {
    background-color: cyan;
    opacity: 0.9;
}
</style>
